{
  "name": "swiss-pairing",
  "version": "1.0.0",
  "description": "A simple CLI tool for generating Swiss pairings",
  "main": "dist/index.js",
  "author": "Arlo Armstrong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arlophoenix/swiss-pairing.git"
  },
  "bin": {
    "swiss-pairing": "./dist/index.js"
  },
  "config": {
    "sourceGlob": "src/**/*.{js,mjs,cjs,ts,test.ts}"
  },
  "scripts": {
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "lint-staged": "lint-staged",
    "eslint": "eslint \"$npm_package_config_sourceGlob\"",
    "eslint:fix": "eslint \"$npm_package_config_sourceGlob\" --fix",
    "prettier": "prettier --check \"$npm_package_config_sourceGlob\"",
    "prettier:fix": "prettier --write \"$npm_package_config_sourceGlob\""
  },
  "lint-staged": {
    "$npm_package_config_sourceGlob": "npm run lint:fix"
  },
  "keywords": [],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "commander": "^12.1.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "type": "module"
}
