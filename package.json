  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arlophoenix/swiss-pairing.git"
  },
  "bin": {
    "swiss-pairing": "./dist/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest",
