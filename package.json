{
  "name": "swiss-pairing",
  "version": "1.0.0",
  "description": "A simple CLI tool for generating Swiss pairings",
  "main": "dist/index.js",
  "author": "Arlo Armstrong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arlophoenix/swiss-pairing.git"
  },
  "bin": {
    "swiss-pairing": "./dist/index.js"
  },
  "engines": {
    "node": "^20.17.0"
  },
  "config": {
    "sourceGlob": "{*.{js,mjs,md},{scripts,src,test}/**/*.{ts,test.ts}}"
  },
  "scripts": {
    "prepare": "husky install && npm run build",
    "preinstall": "npm run update-nvmrc",
    "prepublishOnly": "npm run lint && npm test",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "npm run eslint && npm run prettier && npm run todo",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "lint-staged": "lint-staged",
    "eslint": "eslint \"$npm_package_config_sourceGlob\"",
    "eslint:fix": "eslint \"$npm_package_config_sourceGlob\" --fix",
    "prettier": "prettier --check \"$npm_package_config_sourceGlob\"",
    "prettier:fix": "prettier --write \"$npm_package_config_sourceGlob\"",
    "todo": "leasot \"$npm_package_config_sourceGlob\"",
    "todo:report": "leasot -x --reporter markdown \"$npm_package_config_sourceGlob\" > LEASOT.md",
    "update-nvmrc": "tsx scripts/update-nvmrc.ts",
    "usage": "tsx src/index.js --help",
    "usage:update-readme": "tsx scripts/update-readme-usage.ts"
  },
  "lint-staged": {
    "$npm_package_config_sourceGlob": "npm run lint:fix"
  },
  "keywords": [],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "commander": "^12.1.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "leasot": "^14.4.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "type": "module"
}
