{
  "name": "swiss-pairing",
  "version": "1.0.0",
  "description": "A simple CLI tool for generating Swiss pairings",
  "main": "dist/index.js",
  "author": "Arlo Armstrong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arlophoenix/swiss-pairing.git"
  },
  "bin": {
    "swiss-pairing": "./dist/index.js"
  },
  "engines": {
    "node": "^20.17.0"
  },
  "config": {
    "sourceGlob": "{*.{js,mjs,md},{scripts,src,test}/**/*.{ts,test.ts}}"
  },
  "scripts": {
    "prepare": "husky install && npm run build",
    "preinstall": "npm run update-nvmrc",
    "prepublishOnly": "npm run lint && npm test",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "setup-env": "tsx scripts/setup-env.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "npm run pretest && jest --watch --testPathIgnorePatterns=integration.test.ts",
    "test:unit": "npm run pretest && jest --testPathIgnorePatterns=integration.test.ts",
    "test:integration": "npm run pretest && jest --testPathPattern=integration.test.ts",
    "test:coverage": "npm run pretest && jest --coverage",
    "test:perf": "npm run pretest && tsx scripts/performance-test.ts",
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "npm run eslint && npm run format && npm run todo",
    "lint:fix": "npm run eslint:fix && npm run format:fix",
    "lint-staged": "lint-staged",
    "eslint": "eslint \"$npm_package_config_sourceGlob\"",
    "eslint:fix": "eslint \"$npm_package_config_sourceGlob\" --fix",
    "format": "prettier --check --ignore-unknown **/*",
    "format:fix": "prettier --write --ignore-unknown **/*",
    "readme:update-usage": "tsx scripts/update-readme-usage.ts",
    "readme:update-examples": "tsx scripts/update-readme-examples.ts",
    "todo": "leasot \"$npm_package_config_sourceGlob\"",
    "todo:report": "leasot -x --reporter markdown \"$npm_package_config_sourceGlob\" > LEASOT.md",
    "update-nvmrc": "tsx scripts/update-nvmrc.ts",
    "usage": "tsx src/index.js --help"
  },
  "lint-staged": {
    "$npm_package_config_sourceGlob": "npm run eslint:fix",
    "**/*": "npm run format:fix"
  },
  "keywords": [],
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/debug": "^4.1.12",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.4",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.2",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "leasot": "^14.4.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "type": "module",
  "dependencies": {
    "commander": "^12.1.0",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "file-type": "^19.5.0",
    "papaparse": "^5.4.1",
    "posthog-node": "^4.2.1"
  }
}
